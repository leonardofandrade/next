{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [    
        {
            "name": "NEXT - Run Server",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\manage.py"
        },
        {
            "name": "NEXT - Load Initial Data",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "load_initial_data"
            ],
            "django": true,
            "program": "${workspaceFolder}\\manage.py"
        },
        {
            "name": "NEXT - Create Test Extraction Requests (Interactive)",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "extraction_request_create_test_data",
                "--u",
                "${input:extractionRequestUser}",
                "--a",
                "${input:extractionRequestAmount}",
                "--d",
                "${input:extractionRequestMaxDevices}"
            ],
            "django": true,
            "program": "${workspaceFolder}\\manage.py",
            "console": "integratedTerminal"
        },
        {
            "name": "NEXT - Create Test Extraction Requests (Quick - admin, 13, 1)",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "extraction_request_create_test_data",
                "--u",
                "admin",
                "--a",
                "13",
                "--d",
                "1"
            ],
            "django": true,
            "program": "${workspaceFolder}\\manage.py",
            "console": "integratedTerminal"
        }
    ]
}